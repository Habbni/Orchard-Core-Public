<div id="wf-edit-modal" class="modal" data-bs-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="activity-picker-label">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content" style="height:800px;">
@*             <div class="modal-header">                
                <h2>Edit</h2>
            </div> *@
            <div class="modal-body">
                <i class="fas fa-spinner fa-spin"></i>
                <iframe src="" id="wf-iframe" frameborder="0" style="width:100%;height:100%;"></iframe>
            </div>
            <div class="modal-footer">
                <button class="btn btn-primary save" id="btn-save-wf-edit-modal">@T["Save"]</button>
                <button class="btn btn-secondary cancel" role="button" id="btn-close-wf-edit-modal">@T["Cancel"]</button>
            </div>
        </div>
    </div>
</div>

<script depends-on="jquery,bootstrap,jsplumb" asp-src="~/Grosshaus.Workflows.Replenish/Scripts/wf-modal.js" debug-src="~/Grosshaus.Workflows.Replenish/Scripts/wf-modal.js" at="Foot" defer></script>